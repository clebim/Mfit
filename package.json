{
  "name": "m-fit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev tsx watch ./src/main/server.ts",
    "clean": "rimraf dist/*",
    "start": "node ./dist/main/server.js",
    "build": "npm run clean && tsup",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "migrate:dev": "dotenv -e ./env/.env.dev -- npx prisma migrate dev",
    "docker:setup": "docker volume create postgresql_data",
    "db:studio": "dotenv -e ./env/.env.dev -- npx prisma studio",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prisma": "^4.13.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.17.0",
    "flatted": "^3.2.7",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  }
}
